{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.1",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-libxml": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "astrotomic/laravel-translatable": "^11.12",
        "darkaonline/l5-swagger": "^8.5",
        "guzzlehttp/guzzle": "^7.2",
        "henriqueramos/laravel_json_schema_validator": "^1.0",
        "illuminatech/multipart-middleware": "^1.1",
        "intervention/image": "^2.7",
        "jeroennoten/laravel-adminlte": "^3.8",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "kraken-io/kraken-php": "^1.6",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/csv": "^9.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/iso3166": "^4.2",
        "php-open-source-saver/jwt-auth": "^2.1",
        "predis/predis": "^2.1",
        "propaganistas/laravel-phone": "^5.0",
        "s-ichikawa/laravel-sendgrid-driver": "^4.0",
        "sendgrid/sendgrid": "^8.0",
        "spatie/laravel-backup": "^8.1",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^10.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/common": "^3.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mbezhanov/laravel-faker-provider-collection": "^2.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "phpro/grumphp": "^1.15",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^11.2",
        "xvladqt/faker-lorem-flickr": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
